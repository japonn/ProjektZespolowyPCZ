@{
    ViewData["Title"] = "Panel Właściciela";
}

<div class="container mt-5">
    <div class="row justify-content-center">
        <div class="col-md-8">
            <div class="card">
                <div class="card-body text-center">
                    <h1>Jesteś zalogowany jako: Owner</h1>
                    <p class="lead mt-3">Witaj, @User.Identity?.Name!</p>
                    <a asp-controller="Owner"
                        asp-action="Clients"
                        class="btn btn-primary mt-4">
                        Zobacz listę klientów
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>
